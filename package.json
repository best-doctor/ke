  "scripts": {
    "build": "rollup --config ./rollup.config.js",
    "start": "rollup --config ./rollup.config.js --watch",
    "lint": "eslint --ext .ts,.tsx src",
    "test": "jest --env=jsdom",
    "install-peers": "install-peers",
    "format": "prettier --write 'src/**/*'",
    "typecheck": "yarn tsc --noEmit",
    "makecheck": "yarn typecheck && yarn lint && yarn test && mdl README.md && mdl docs",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "typedoc --out docs/generated/typedoc  src/index.tsx",
    "release": "standard-version",
    "release:dry": "standard-version --dry-run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs/generated/storybook",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "install-peers"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
