        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "lint-staged": "^10.2.2",
        "prettier": "^2.0.4",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-test-renderer": "^16.13.1",
